{
  "kind": "collectionType",
  "collectionName": "manifestos",
  "info": {
    "singularName": "manifesto",
    "pluralName": "manifestos",
    "displayName": "Manifesto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "default": "-"
    },
    "CoverImages": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Images": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Author": {
      "type": "string",
      "default": "-"
    },
    "Year": {
      "type": "integer"
    },
    "Content": {
      "type": "richtext",
      "default": "-"
    },
    "ThContent": {
      "type": "richtext",
      "default": "-"
    },
    "ThAuthor": {
      "type": "string",
      "default": "-",
      "required": true
    },
    "Description": {
      "type": "text",
      "default": "-"
    },
    "ThDescription": {
      "type": "text",
      "default": "-"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "manifestos"
    },
    "ThTitle": {
      "type": "string",
      "default": "-"
    },
    "TextAlign": {
      "type": "enumeration",
      "enum": [
        "Left",
        "Center",
        "Right"
      ],
      "default": "Left"
    },
    "PublishDate": {
      "type": "date",
      "default": "2022-01-01"
    },
    "DownloadLink": {
      "type": "string",
      "default": "-",
      "required": true
    },
    "BodyImages": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
